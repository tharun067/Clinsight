      "minTouchTarget": "44px",
      
      "contrastRatios": {
        "normalText": "4.5:1",
        "largeText": "3:1",
        "uiComponents": "3:1"
      }
    },

    "usageGuidelines": {
      "colorApplication": {
        "cards": {
          "background": "#FFFFFF",
          "border": "none or 1px solid #E5E7EB",
