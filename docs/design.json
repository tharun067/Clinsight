{
  "designSystem": {
    "metadata": {
      "name": "Medical AI Diagnostic Assistant Design System",
      "version": "1.0.0",
      "description": "Comprehensive design system extracted from hospital management interface for multi-modal RAG medical diagnosis application",
      "extractedFrom": "Hospital Management System UI (Dribbble)",
      "targetFramework": "React with Tailwind CSS"
    },

    "colorPalette": {
      "primary": {
        "teal": {
          "50": "#E6F7F5",
          "100": "#B3E8E1",
          "200": "#80D9CD",
          "300": "#4DCAB9",
          "400": "#1ABBA5",
          "500": "#14B8A6",
          "600": "#10A693",
          "700": "#0D9480",
          "800": "#0A826D",
          "900": "#07705A",
          "description": "Primary brand color - used for main actions, active states, key UI elements"
        }
      },
      
      "semantic": {
        "success": {
          "50": "#E8F5E9",
          "100": "#C8E6C9",
          "500": "#4CAF50",
          "600": "#43A047",
          "700": "#388E3C",
          "usage": "Confirmed appointments, completed status, positive metrics"
        },
        "warning": {
          "50": "#FFF8E1",
          "100": "#FFECB3",
          "500": "#FFA726",
          "600": "#FB8C00",
          "700": "#F57C00",
          "usage": "Pending status, due soon alerts, medium priority"
        },
        "danger": {
          "50": "#FFEBEE",
          "100": "#FFCDD2",
          "500": "#EF5350",
          "600": "#E53935",
          "700": "#D32F2F",
          "usage": "Overdue payments, canceled appointments, errors, critical alerts"
        },
        "info": {
          "50": "#E3F2FD",
          "100": "#BBDEFB",
          "500": "#2196F3",
          "600": "#1E88E5",
          "700": "#1976D2",
          "usage": "Information alerts, available status, general notifications"
        }
      },

      "neutral": {
        "white": "#FFFFFF",
        "gray": {
          "50": "#F9FAFB",
          "100": "#F3F4F6",
          "200": "#E5E7EB",
          "300": "#D1D5DB",
          "400": "#9CA3AF",
          "500": "#6B7280",
          "600": "#4B5563",
          "700": "#374151",
          "800": "#1F2937",
          "900": "#111827"
        },
        "offWhite": "#F9FAFB",
        "lightBeige": "#F3F8FD"
      },

      "chartColors": {
        "revenue": "#8B5A3C",
        "expenses": "#5B8EC4",
        "antibiotics": "#5B8EC4",
        "painRelief": "#4CAF50",
        "diabetes": "#9C27B0",
        "bloodPressure": "#FFA726",
        "others": "#78909C"
      }
    },

    "typography": {
      "fontFamily": {
        "primary": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
        "fallback": "system-ui, sans-serif"
      },
      
      "fontSizes": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem"
      },

      "fontWeights": {
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700"
      },

      "lineHeights": {
        "tight": "1.25",
        "normal": "1.5",
        "relaxed": "1.75"
      },

      "textStyles": {
        "pageTitle": {
          "fontSize": "2.25rem",
          "fontWeight": "700",
          "lineHeight": "1.25",
          "color": "#111827",
          "usage": "Main page headings (e.g., 'Dashboard Overview')"
        },
        "sectionTitle": {
          "fontSize": "1.5rem",
          "fontWeight": "600",
          "lineHeight": "1.5",
          "color": "#1F2937",
          "usage": "Section headers within pages"
        },
        "cardTitle": {
          "fontSize": "1rem",
          "fontWeight": "600",
          "lineHeight": "1.5",
          "color": "#1F2937",
          "usage": "Card headers, component titles"
        },
        "bodyLarge": {
          "fontSize": "1rem",
          "fontWeight": "400",
          "lineHeight": "1.5",
          "color": "#374151"
        },
        "bodyNormal": {
          "fontSize": "0.875rem",
          "fontWeight": "400",
          "lineHeight": "1.5",
          "color": "#4B5563"
        },
        "bodySmall": {
          "fontSize": "0.75rem",
          "fontWeight": "400",
          "lineHeight": "1.5",
          "color": "#6B7280"
        },
        "label": {
          "fontSize": "0.75rem",
          "fontWeight": "500",
          "lineHeight": "1.5",
          "color": "#6B7280",
          "textTransform": "uppercase",
          "letterSpacing": "0.05em"
        },
        "caption": {
          "fontSize": "0.75rem",
          "fontWeight": "400",
          "lineHeight": "1.5",
          "color": "#9CA3AF"
        }
      }
    },

    "spacing": {
      "scale": {
        "0": "0",
        "1": "0.25rem",
        "2": "0.5rem",
        "3": "0.75rem",
        "4": "1rem",
        "5": "1.25rem",
        "6": "1.5rem",
        "8": "2rem",
        "10": "2.5rem",
        "12": "3rem",
        "16": "4rem",
        "20": "5rem"
      },
      
      "usage": {
        "containerPadding": "1.5rem",
        "cardPadding": "1.5rem",
        "cardGap": "1.5rem",
        "sectionGap": "2rem",
        "elementGap": "1rem",
        "tightGap": "0.5rem"
      }
    },

    "borderRadius": {
      "none": "0",
      "sm": "0.25rem",
      "base": "0.5rem",
      "md": "0.75rem",
      "lg": "1rem",
      "xl": "1.5rem",
      "full": "9999px",
      
      "usage": {
        "cards": "1rem",
        "buttons": "0.5rem",
        "inputs": "0.5rem",
        "badges": "9999px",
        "avatars": "9999px",
        "modals": "1rem"
      }
    },

    "shadows": {
      "none": "none",
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      
      "usage": {
        "cards": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
        "cardsHover": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
        "modals": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
        "buttons": "0 1px 2px 0 rgba(0, 0, 0, 0.05)"
      }
    },

    "components": {
      "button": {
        "variants": {
          "primary": {
            "default": {
              "backgroundColor": "#14B8A6",
              "color": "#FFFFFF",
              "borderRadius": "0.5rem",
              "padding": "0.625rem 1.25rem",
              "fontSize": "0.875rem",
              "fontWeight": "500",
              "border": "none",
              "boxShadow": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
              "transition": "all 0.2s ease"
            },
            "hover": {
              "backgroundColor": "#10A693",
              "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
            },
            "active": {
              "backgroundColor": "#0D9480"
            },
            "disabled": {
              "backgroundColor": "#9CA3AF",
              "cursor": "not-allowed",
              "opacity": "0.6"
            }
          },
          
          "secondary": {
            "default": {
              "backgroundColor": "#FFFFFF",
              "color": "#4B5563",
              "borderRadius": "0.5rem",
              "padding": "0.625rem 1.25rem",
              "fontSize": "0.875rem",
              "fontWeight": "500",
              "border": "1px solid #E5E7EB",
              "transition": "all 0.2s ease"
            },
            "hover": {
              "backgroundColor": "#F9FAFB",
              "borderColor": "#D1D5DB"
            }
          },
          
          "ghost": {
            "default": {
              "backgroundColor": "transparent",
              "color": "#14B8A6",
              "borderRadius": "0.5rem",
              "padding": "0.625rem 1.25rem",
              "fontSize": "0.875rem",
              "fontWeight": "500",
              "border": "none"
            },
            "hover": {
              "backgroundColor": "#E6F7F5"
            }
          },
          
          "icon": {
            "default": {
              "backgroundColor": "transparent",
              "color": "#6B7280",
              "borderRadius": "0.5rem",
              "padding": "0.5rem",
              "border": "none",
              "width": "2.5rem",
              "height": "2.5rem",
              "display": "flex",
              "alignItems": "center",
              "justifyContent": "center"
            },
            "hover": {
              "backgroundColor": "#F3F4F6",
              "color": "#374151"
            }
          }
        },
        
        "withIcon": {
          "iconPosition": "left | right",
          "iconGap": "0.5rem",
          "iconSize": "1rem"
        }
      },

      "card": {
        "default": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "1rem",
          "padding": "1.5rem",
          "border": "none",
          "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
          "transition": "all 0.2s ease"
        },
        "hover": {
          "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
          "transform": "translateY(-2px)"
        },
        
        "statCard": {
          "layout": "flex column",
          "gap": "0.75rem",
          "minHeight": "120px",
          "header": {
            "display": "flex",
            "justifyContent": "space-between",
            "alignItems": "flex-start"
          },
          "icon": {
            "width": "3rem",
            "height": "3rem",
            "borderRadius": "0.75rem",
            "display": "flex",
            "alignItems": "center",
            "justifyContent": "center",
            "fontSize": "1.25rem"
          },
          "iconColors": {
            "blue": {
              "background": "#DBEAFE",
              "icon": "#2563EB"
            },
            "green": {
              "background": "#D1FAE5",
              "icon": "#059669"
            },
            "orange": {
              "background": "#FED7AA",
              "icon": "#EA580C"
            },
            "teal": {
              "background": "#CCFBF1",
              "icon": "#0D9488"
            }
          },
          "value": {
            "fontSize": "1.875rem",
            "fontWeight": "700",
            "color": "#111827"
          },
          "label": {
            "fontSize": "0.875rem",
            "fontWeight": "400",
            "color": "#6B7280"
          },
          "change": {
            "fontSize": "0.75rem",
            "fontWeight": "500",
            "display": "flex",
            "alignItems": "center",
            "gap": "0.25rem",
            "positive": {
              "color": "#059669"
            },
            "negative": {
              "color": "#DC2626"
            }
          }
        },
        
        "listCard": {
          "padding": "1.25rem",
          "header": {
            "display": "flex",
            "justifyContent": "space-between",
            "alignItems": "center",
            "marginBottom": "1rem"
          },
          "item": {
            "padding": "0.75rem 0",
            "borderBottom": "1px solid #F3F4F6",
            "lastChild": {
              "borderBottom": "none"
            }
          }
        }
      },

      "badge": {
        "variants": {
          "confirmed": {
            "backgroundColor": "#D1FAE5",
            "color": "#065F46",
            "borderRadius": "9999px",
            "padding": "0.25rem 0.75rem",
            "fontSize": "0.75rem",
            "fontWeight": "500"
          },
          "pending": {
            "backgroundColor": "#FEF3C7",
            "color": "#92400E",
            "borderRadius": "9999px",
            "padding": "0.25rem 0.75rem",
            "fontSize": "0.75rem",
            "fontWeight": "500"
          },
          "overdue": {
            "backgroundColor": "#FEE2E2",
            "color": "#991B1B",
            "borderRadius": "9999px",
            "padding": "0.25rem 0.75rem",
            "fontSize": "0.75rem",
            "fontWeight": "500"
          },
          "canceled": {
            "backgroundColor": "#FEE2E2",
            "color": "#991B1B",
            "borderRadius": "9999px",
            "padding": "0.25rem 0.75rem",
            "fontSize": "0.75rem",
            "fontWeight": "500"
          },
          "available": {
            "backgroundColor": "#DBEAFE",
            "color": "#1E40AF",
            "borderRadius": "9999px",
            "padding": "0.25rem 0.75rem",
            "fontSize": "0.75rem",
            "fontWeight": "500"
          },
          "online": {
            "backgroundColor": "#D1FAE5",
            "color": "#065F46",
            "borderRadius": "9999px",
            "padding": "0.25rem 0.75rem",
            "fontSize": "0.75rem",
            "fontWeight": "500",
            "icon": {
              "width": "0.5rem",
              "height": "0.5rem",
              "backgroundColor": "#059669",
              "borderRadius": "9999px",
              "marginRight": "0.375rem"
            }
          }
        },
        
        "withIcon": {
          "display": "flex",
          "alignItems": "center",
          "gap": "0.375rem"
        }
      },

      "input": {
        "default": {
          "backgroundColor": "#FFFFFF",
          "border": "1px solid #D1D5DB",
          "borderRadius": "0.5rem",
          "padding": "0.625rem 0.875rem",
          "fontSize": "0.875rem",
          "color": "#111827",
          "transition": "all 0.2s ease"
        },
        "focus": {
          "borderColor": "#14B8A6",
          "outline": "2px solid rgba(20, 184, 166, 0.1)",
          "outlineOffset": "0"
        },
        "error": {
          "borderColor": "#EF4444"
        },
        "disabled": {
          "backgroundColor": "#F9FAFB",
          "color": "#9CA3AF",
          "cursor": "not-allowed"
        },
        
        "searchInput": {
          "paddingLeft": "2.5rem",
          "icon": {
            "position": "absolute",
            "left": "0.875rem",
            "top": "50%",
            "transform": "translateY(-50%)",
            "color": "#9CA3AF",
            "fontSize": "1rem"
          }
        }
      },

      "dropdown": {
        "default": {
          "backgroundColor": "#FFFFFF",
          "border": "1px solid #D1D5DB",
          "borderRadius": "0.5rem",
          "padding": "0.625rem 0.875rem",
          "fontSize": "0.875rem",
          "color": "#111827",
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "center",
          "cursor": "pointer"
        },
        "icon": {
          "color": "#6B7280",
          "fontSize": "1rem"
        }
      },

      "avatar": {
        "sizes": {
          "xs": {
            "width": "1.5rem",
            "height": "1.5rem",
            "fontSize": "0.625rem"
          },
          "sm": {
            "width": "2rem",
            "height": "2rem",
            "fontSize": "0.75rem"
          },
          "md": {
            "width": "2.5rem",
            "height": "2.5rem",
            "fontSize": "0.875rem"
          },
          "lg": {
            "width": "3.5rem",
            "height": "3.5rem",
            "fontSize": "1.125rem"
          },
          "xl": {
            "width": "4.5rem",
            "height": "4.5rem",
            "fontSize": "1.5rem"
          }
        },
        "style": {
          "borderRadius": "9999px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "fontWeight": "600",
          "backgroundColor": "#14B8A6",
          "color": "#FFFFFF"
        },
        "withImage": {
          "objectFit": "cover"
        },
        "withOnlineIndicator": {
          "position": "relative",
          "indicator": {
            "position": "absolute",
            "bottom": "0",
            "right": "0",
            "width": "0.75rem",
            "height": "0.75rem",
            "backgroundColor": "#10B981",
            "border": "2px solid #FFFFFF",
            "borderRadius": "9999px"
          }
        }
      },

      "sidebar": {
        "container": {
          "width": "16rem",
          "backgroundColor": "#FFFFFF",
          "borderRight": "1px solid #E5E7EB",
          "height": "100vh",
          "padding": "1.5rem 0",
          "display": "flex",
          "flexDirection": "column"
        },
        
        "logo": {
          "padding": "0 1.5rem",
          "marginBottom": "2rem",
          "display": "flex",
          "alignItems": "center",
          "gap": "0.75rem",
          "icon": {
            "width": "2.5rem",
            "height": "2.5rem",
            "backgroundColor": "#14B8A6",
            "borderRadius": "0.5rem",
            "color": "#FFFFFF",
            "display": "flex",
            "alignItems": "center",
            "justifyContent": "center"
          },
          "text": {
            "fontSize": "1.125rem",
            "fontWeight": "600",
            "color": "#111827"
          }
        },
        
        "menuItem": {
          "default": {
            "padding": "0.75rem 1.5rem",
            "display": "flex",
            "alignItems": "center",
            "gap": "0.75rem",
            "color": "#6B7280",
            "fontSize": "0.875rem",
            "fontWeight": "500",
            "cursor": "pointer",
            "transition": "all 0.2s ease"
          },
          "hover": {
            "backgroundColor": "#F9FAFB",
            "color": "#111827"
          },
          "active": {
            "backgroundColor": "#E6F7F5",
            "color": "#14B8A6",
            "borderLeft": "3px solid #14B8A6"
          },
          "icon": {
            "fontSize": "1.25rem"
          }
        },
        
        "userSection": {
          "marginTop": "auto",
          "padding": "1.5rem",
          "borderTop": "1px solid #E5E7EB",
          "display": "flex",
          "alignItems": "center",
          "gap": "0.75rem"
        }
      },

      "header": {
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderBottom": "1px solid #E5E7EB",
          "padding": "1rem 1.5rem",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "space-between"
        },
        
        "pageHeader": {
          "icon": {
            "width": "2.5rem",
            "height": "2.5rem",
            "backgroundColor": "#14B8A6",
            "borderRadius": "0.75rem",
            "color": "#FFFFFF",
            "display": "flex",
            "alignItems": "center",
            "justifyContent": "center",
            "fontSize": "1.25rem"
          },
          "title": {
            "fontSize": "1.25rem",
            "fontWeight": "600",
            "color": "#111827"
          },
          "subtitle": {
            "fontSize": "0.875rem",
            "fontWeight": "400",
            "color": "#6B7280"
          }
        }
      },

      "table": {
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "1rem",
          "overflow": "hidden",
          "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1)"
        },
        
        "header": {
          "backgroundColor": "#F9FAFB",
          "borderBottom": "1px solid #E5E7EB"
        },
        
        "headerCell": {
          "padding": "0.75rem 1rem",
          "fontSize": "0.75rem",
          "fontWeight": "600",
          "color": "#6B7280",
          "textTransform": "uppercase",
          "letterSpacing": "0.05em"
        },
        
        "row": {
          "borderBottom": "1px solid #F3F4F6",
          "transition": "background-color 0.2s ease",
          "hover": {
            "backgroundColor": "#F9FAFB"
          }
        },
        
        "cell": {
          "padding": "1rem",
          "fontSize": "0.875rem",
          "color": "#374151"
        }
      },

      "calendar": {
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "1rem",
          "padding": "1.5rem"
        },
        
        "header": {
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "center",
          "marginBottom": "1rem"
        },
        
        "dayHeader": {
          "fontSize": "0.75rem",
          "fontWeight": "600",
          "color": "#6B7280",
          "textAlign": "center",
          "padding": "0.5rem"
        },
        
        "day": {
          "default": {
            "padding": "0.5rem",
            "textAlign": "center",
            "fontSize": "0.875rem",
            "color": "#374151",
            "borderRadius": "0.5rem",
            "cursor": "pointer"
          },
          "hover": {
            "backgroundColor": "#F3F4F6"
          },
          "today": {
            "backgroundColor": "#E6F7F5",
            "color": "#14B8A6",
            "fontWeight": "600"
          },
          "hasAppointment": {
            "position": "relative",
            "indicator": {
              "position": "absolute",
              "bottom": "0.25rem",
              "left": "50%",
              "transform": "translateX(-50%)",
              "width": "0.25rem",
              "height": "0.25rem",
              "borderRadius": "9999px"
            }
          }
        },
        
        "appointment": {
          "padding": "0.5rem",
          "borderRadius": "0.5rem",
          "fontSize": "0.75rem",
          "marginBottom": "0.25rem",
          "confirmed": {
            "backgroundColor": "#D1FAE5",
            "color": "#065F46"
          },
          "pending": {
            "backgroundColor": "#FEF3C7",
            "color": "#92400E"
          },
          "canceled": {
            "backgroundColor": "#FEE2E2",
            "color": "#991B1B"
          }
        }
      },

      "modal": {
        "overlay": {
          "backgroundColor": "rgba(0, 0, 0, 0.5)",
          "position": "fixed",
          "inset": "0",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "zIndex": "50"
        },
        
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "1rem",
          "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
          "maxWidth": "32rem",
          "width": "100%",
          "padding": "1.5rem"
        },
        
        "header": {
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "center",
          "marginBottom": "1rem"
        },
        
        "title": {
          "fontSize": "1.25rem",
          "fontWeight": "600",
          "color": "#111827"
        }
      },

      "progressBar": {
        "container": {
          "width": "100%",
          "height": "0.5rem",
          "backgroundColor": "#E5E7EB",
          "borderRadius": "9999px",
          "overflow": "hidden"
        },
        
        "fill": {
          "height": "100%",
          "borderRadius": "9999px",
          "transition": "width 0.3s ease",
          "variants": {
            "success": {
              "backgroundColor": "#10B981"
            },
            "warning": {
              "backgroundColor": "#F59E0B"
            },
            "danger": {
              "backgroundColor": "#EF4444"
            },
            "info": {
              "backgroundColor": "#3B82F6"
            }
          }
        }
      },

      "alert": {
        "variants": {
          "success": {
            "backgroundColor": "#D1FAE5",
            "borderLeft": "4px solid #10B981",
            "color": "#065F46",
            "icon": {
              "color": "#10B981"
            }
          },
          "warning": {
            "backgroundColor": "#FEF3C7",
            "borderLeft": "4px solid #F59E0B",
            "color": "#92400E",
            "icon": {
              "color": "#F59E0B"
            }
          },
          "danger": {
            "backgroundColor": "#FEE2E2",
            "borderLeft": "4px solid #EF4444",
            "color": "#991B1B",
            "icon": {
              "color": "#EF4444"
            }
          },
          "info": {
            "backgroundColor": "#DBEAFE",
            "borderLeft": "4px solid #3B82F6",
            "color": "#1E3A8A",
            "icon": {
              "color": "#3B82F6"
            }
          }
        },
        
        "common": {
          "padding": "1rem",
          "borderRadius": "0.5rem",
          "display": "flex",
          "alignItems": "flex-start",
          "gap": "0.75rem",
          "fontSize": "0.875rem"
        }
      },

      "doctorCard": {
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "1rem",
          "padding": "1.5rem",
          "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1)"
        },
        
        "header": {
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "flex-start",
          "marginBottom": "1rem"
        },
        
        "avatar": {
          "width": "3.5rem",
          "height": "3.5rem",
          "borderRadius": "9999px",
          "objectFit": "cover"
        },
        
        "name": {
          "fontSize": "1rem",
          "fontWeight": "600",
          "color": "#111827"
        },
        
        "specialty": {
          "fontSize": "0.875rem",
          "color": "#6B7280"
        },
        
        "statusBadge": {
          "display": "flex",
          "alignItems": "center",
          "gap": "0.375rem",
          "padding": "0.25rem 0.75rem",
          "borderRadius": "9999px",
          "fontSize": "0.75rem",
          "fontWeight": "500"
        },
        
        "schedule": {
          "marginTop": "1rem",
          "day": {
            "display": "flex",
            "justifyContent": "space-between",
            "padding": "0.5rem 0",
            "fontSize": "0.875rem",
            "borderBottom": "1px solid #F3F4F6"
          }
        }
      },

      "patientCard": {
        "container": {
          "display": "flex",
          "alignItems": "center",
          "gap": "1rem",
          "padding": "1rem",
          "borderRadius": "0.75rem",
          "cursor": "pointer",
          "transition": "background-color 0.2s ease",
          "hover": {
            "backgroundColor": "#F9FAFB"
          }
        },
        
        "avatar": {
          "width": "3rem",
          "height": "3rem",
          "borderRadius": "9999px",
          "flexShrink": "0"
        },
        
        "info": {
          "flex": "1"
        },
        
        "name": {
          "fontSize": "0.875rem",
          "fontWeight": "600",
          "color": "#111827"
        },
        
        "meta": {
          "fontSize": "0.75rem",
          "color": "#6B7280",
          "display": "flex",
          "gap": "0.5rem"
        }
      },

      "appointmentTimeline": {
        "item": {
          "display": "flex",
          "gap": "1rem",
          "padding": "0.75rem 0"
        },
        
        "time": {
          "fontSize": "0.875rem",
          "fontWeight": "500",
          "color": "#6B7280",
          "minWidth": "5rem"
        },
        
        "card": {
          "flex": "1",
          "padding": "1rem",
          "borderRadius": "0.75rem",
          "borderLeft": "4px solid"
        }
      },

      "filter": {
        "container": {
          "display": "flex",
          "alignItems": "center",
          "gap": "0.5rem",
          "padding": "0.5rem 0.75rem",
          "borderRadius": "0.5rem",
          "border": "1px solid #E5E7EB",
          "backgroundColor": "#FFFFFF",
          "cursor": "pointer"
        },
        
        "icon": {
          "color": "#6B7280",
          "fontSize": "1rem"
        },
        
        "label": {
          "fontSize": "0.875rem",
          "color": "#374151"
        }
      }
