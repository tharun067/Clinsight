    "layouts": {
      "dashboard": {
        "container": {
          "display": "grid",
          "gridTemplateColumns": "16rem 1fr",
          "minHeight": "100vh",
          "backgroundColor": "#F9FAFB"
        },
        
        "mainContent": {
          "padding": "2rem",
          "overflowY": "auto"
        },
        
        "statsGrid": {
          "display": "grid",
          "gridTemplateColumns": "repeat(auto-fit, minmax(250px, 1fr))",
          "gap": "1.5rem",
          "marginBottom": "2rem"
        },
        
        "contentGrid": {
          "display": "grid",
          "gridTemplateColumns": "2fr 1fr",
          "gap": "1.5rem",
          "marginTop": "2rem"
        }
      },
      
      "detailView": {
        "container": {
          "display": "grid",
          "gridTemplateColumns": "350px 1fr",
          "gap": "1.5rem",
          "padding": "2rem"
        },
        
        "sidebar": {
          "display": "flex",
          "flexDirection": "column",
          "gap": "1.5rem"
        }
      }
    },

    "iconography": {
      "library": "lucide-react",
      "sizes": {
        "sm": "1rem",
        "md": "1.25rem",
        "lg": "1.5rem",
        "xl": "2rem"
      },
      
      "commonIcons": {
